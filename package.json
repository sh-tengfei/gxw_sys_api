{
  "name": "gxw-api",
  "version": "1.0.0",
  "description": "果仙网小程序",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "decimal.js": "^10.2.0",
    "egg": "^2.15.1",
    "egg-alinode": "^2.0.1",
    "egg-curl": "^1.0.1",
    "egg-jwt": "^3.1.7",
    "egg-mongoose": "^3.2.0",
    "egg-scripts": "^2.11.0",
    "mongoose-double": "0.0.1",
    "mongoose-unique-validator": "^2.0.3",
    "nodemailer": "^6.4.6",
    "officegen": "^0.6.4",
    "qiniu": "^7.3.0",
    "request": "^2.88.2",
    "sha1": "^1.1.1",
    "tunnel-ssh": "^4.1.4",
    "wechat-api": "^1.35.1",
    "xml2js": "^0.4.23",
    "xmlreader": "^0.2.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "babel-core": "^6.26.3",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "dev": "egg-bin dev --env=local",
    "dev:start": "egg-scripts start --daemon --env=local --title=local-egg-gxw",
    "dev:stop": "egg-scripts stop --title=local-egg-gxw",

    "test:dev": "egg-bin dev --env=test",
    "test:start": "egg-scripts start --daemon --env=test --title=test-egg-gxw",
    "test:stop": "egg-scripts stop --title=test-egg-gxw",
    "test:restart": "npm run test:stop && npm run test:start",

    "pre:dev": "egg-bin dev --env=pre",
    "pre:start": "egg-scripts start --daemon --env=pre --title=pre-egg-gxw",
    "pre:stop": "egg-scripts stop --title=pre-egg-gxw",
    "pre:restart": "npm run pre:stop && npm run pre:start",

    "prod:dev": "egg-bin dev --env=prod",
    "prod:start": "egg-scripts start --daemon --env=prod --title=prod-egg-gxw",
    "prod:stop": "egg-scripts stop --title=prod-egg-gxw",
    "prod:restart": "npm run prod:stop && npm run prod:start",

    "debug": "egg-bin debug",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .js app config",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": "git@gitee.com:fruit_fairy_net/gxw_sys_api.git"
  },
  "author": "shangtengfei",
  "license": "MIT"
}
